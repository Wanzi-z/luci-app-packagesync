{
  "luci-app-packagesync": {
    "description": "Grant access to packagesync procedures",
    "read": {
      "file": {
        "/etc/init.d/packagesync": [ "exec" ],
        "/bin/df -hT": [ "exec" ],
        "/var/packagesync/releaseslist": [ "read" ]
      },
      "ubus": {
        "service": [ "list" ]
      },
      "uci": [ "packagesync" ]
    },
    "write": {
      "uci": [ "packagesync" ]
    }
  }
}
